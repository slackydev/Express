func main();
  var arr: array of int;
  var n: Int = 10 000 000;
  var sum: Int = 0;
  var before, after: Double;

  arr.SetLen(n);

  // Initialize array
  for(var i: Int = 0; i < n; i := i + 1) do
    arr[i] := i % 1000;
  end

  before := GetTickCount();

  for(var i: Int = 0; i < n; i := i + 1) do
    sum := sum + arr[i]
  end

  after := GetTickCount();

  print after - before;
  print i
  print sum;
end;

main();