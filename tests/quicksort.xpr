type TIntArray = array of Int64;

func TIntArray.QuickSort(left, right: Int64);
  if(left >= right) return;

  var pivot := self[(left + right) / 2]
  var i := left
  var j := right
  
  while(i <= j) do
    while(self[i] < pivot) i += 1
    while(self[j] > pivot) j -= 1

    if(i <= j) then
      var temp := self[i]
      self[i] := self[j]
      self[j] := temp
      i += 1
      j -= 1
    end;
  end;

  if(left < j)  self.QuickSort(left, j);
  if(i < right) self.QuickSort(i, right);
end;

func main();
  var arr: TIntArray;
  arr.SetLen(1000000)
  
  var n:= arr.High() + 1
  
  for(var i:=0; i<n; i:=i+1)
    arr[i] := RandInt(0, n)
    

  print arr[arr.High()]
  arr.Quicksort(0, arr.High());
end

main()
