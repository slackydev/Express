// This piece of code actually puts the language to a full test of nearly all basics
// Original work: https://villavu.com/forum/showthread.php?t=90383

func main()
  var n: Int32   = 5000;
  var len: Int32 = 10 * n / 3;
  var i, j, k, nines, predigit: int32 = 0;
  var a, res: TIntArray = nil;
  var resCount: Int32 = 0;
  
  a.SetLen(len+1);
  res.SetLen(n+1);
  
  var tm, tma:Double;
  tm := GetTickCount();
  
  for(j:=0; j<=len; j:=j+1) 
    a[j] := 2;  //Start with 2s
  
  for(j:=0; j<n; j:=j+1) do
    var q: Int32 = 0;
    for(i:=len; i>0; i -= 1) do         //Work backwards
      var x:int32 = 10*a[i-1] + q*i;
      a[i-1] := x % (2 * i - 1);
      q      := x / (2 * i - 1);
    end;    
    
    a[0] := q % 10;
    q    := q / 10;
    
    if(q = 9) then
      nines += 1
    elif(q = 10) then
      res[resCount] := predigit+1;
      resCount += 1;
      
      for(k:=1; k<=nines; k+=1) do
        res[resCount] := 0;            //zeros
        resCount += 1;
      end
      predigit := 0
      nines := 0
    else
      res[resCount] := predigit;
      resCount += 1;
      predigit := q;
      if(nines != 0) then
        for(k:=1; k<=nines; k+=1) do
          res[resCount] := 9;
          resCount += 1;
        end;
        nines := 0
      end
    end
  end;
 

  res[resCount] := predigit;
  tma := GetTickCount();
  print tma - tm;
  //for(i:=0; i<=resCount; i:=i+1)
  //  print res[i];
end;

main()
