// --- Arithmetic Operations ---

bcADD, bcSUB, bcMUL, bcDIV, bcMOD,
bcBND, bcBOR, bcXOR, bcEQ, bcNE,
bcGT, bcLT, bcGTE, bcLTE,
bcSAR, bcSHL, bcSHR:
begin
  // Point left operand
  if pc^.Args[0].Pos = mpLocal then
    left := Pointer(BasePtr + pc^.Args[0].Data.Addr)
  else
    left := @pc^.Args[0].Data.Arg;

  // Point right operand
  if pc^.Args[1].Pos = mpLocal then
    right := Pointer(BasePtr + pc^.Args[1].Data.Addr)
  else
    right := @pc^.Args[1].Data.Arg;

  // Type dispatch
  case pc^.Args[0].BaseType of
    xtInt8:
      case pc^.Code of
        bcADD: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt8(left)^ + PInt8(right)^;
        bcSUB: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt8(left)^ - PInt8(right)^;
        bcMUL: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt8(left)^ * PInt8(right)^;
        bcDIV: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt8(left)^ div PInt8(right)^;
        bcMOD: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Modulo(PInt8(left)^, PInt8(right)^);
        bcEQ:  PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt8(left)^ = PInt8(right)^);
        bcNE:  PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt8(left)^ <> PInt8(right)^);
        bcGT:  PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt8(left)^ > PInt8(right)^);
        bcLT:  PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt8(left)^ < PInt8(right)^);
        bcGTE: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt8(left)^ >= PInt8(right)^);
        bcLTE: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt8(left)^ <= PInt8(right)^);
        bcBND: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt8(left)^ and PInt8(right)^;
        bcBOR: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt8(left)^ or  PInt8(right)^;
        bcXOR: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt8(left)^ xor PInt8(right)^;
        bcSAR: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Sar(PInt8(left)^, PInt8(right)^);
        bcSHR: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt8(left)^ shr PInt8(right)^;
        bcSHL: PInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt8(left)^ shl PInt8(right)^;
      end;

    xtUInt8:
      case pc^.Code of
        bcADD: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt8(left)^ + PUInt8(right)^;
        bcSUB: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt8(left)^ - PUInt8(right)^;
        bcMUL: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt8(left)^ * PUInt8(right)^;
        bcDIV: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt8(left)^ div PUInt8(right)^;
        bcMOD: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Modulo(PUInt8(left)^, PUInt8(right)^);
        bcEQ:  PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt8(left)^ = PUInt8(right)^);
        bcNE:  PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt8(left)^ <> PUInt8(right)^);
        bcGT:  PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt8(left)^ > PUInt8(right)^);
        bcLT:  PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt8(left)^ < PUInt8(right)^);
        bcGTE: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt8(left)^ >= PUInt8(right)^);
        bcLTE: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt8(left)^ <= PUInt8(right)^);
        bcBND: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt8(left)^ and PUInt8(right)^;
        bcBOR: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt8(left)^ or  PUInt8(right)^;
        bcXOR: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt8(left)^ xor PUInt8(right)^;
        bcSAR: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Sar(PUInt8(left)^, PUInt8(right)^);
        bcSHR: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt8(left)^ shr PUInt8(right)^;
        bcSHL: PUInt8(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt8(left)^ shl PUInt8(right)^;
      end;

    xtInt16:
      case pc^.Code of
        bcADD: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt16(left)^ + PInt16(right)^;
        bcSUB: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt16(left)^ - PInt16(right)^;
        bcMUL: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt16(left)^ * PInt16(right)^;
        bcDIV: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt16(left)^ div PInt16(right)^;
        bcMOD: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Modulo(PInt16(left)^, PInt16(right)^);
        bcEQ:  PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt16(left)^ = PInt16(right)^);
        bcNE:  PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt16(left)^ <> PInt16(right)^);
        bcGT:  PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt16(left)^ > PInt16(right)^);
        bcLT:  PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt16(left)^ < PInt16(right)^);
        bcGTE: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt16(left)^ >= PInt16(right)^);
        bcLTE: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PInt16(left)^ <= PInt16(right)^);
        bcBND: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt16(left)^ and PInt16(right)^;
        bcBOR: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt16(left)^ or  PInt16(right)^;
        bcXOR: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt16(left)^ xor PInt16(right)^;
        bcSAR: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Sar(PInt16(left)^, PInt16(right)^);
        bcSHR: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt16(left)^ shr PInt16(right)^;
        bcSHL: PInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PInt16(left)^ shl PInt16(right)^;
      end;

    xtUInt16:
      case pc^.Code of
        bcADD: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt16(left)^ + PUInt16(right)^;
        bcSUB: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt16(left)^ - PUInt16(right)^;
        bcMUL: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt16(left)^ * PUInt16(right)^;
        bcDIV: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt16(left)^ div PUInt16(right)^;
        bcMOD: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Modulo(PUInt16(left)^, PUInt16(right)^);
        bcEQ:  PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt16(left)^ = PUInt16(right)^);
        bcNE:  PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt16(left)^ <> PUInt16(right)^);
        bcGT:  PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt16(left)^ > PUInt16(right)^);
        bcLT:  PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt16(left)^ < PUInt16(right)^);
        bcGTE: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt16(left)^ >= PUInt16(right)^);
        bcLTE: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Ord(PUInt16(left)^ <= PUInt16(right)^);
        bcBND: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt16(left)^ and PUInt16(right)^;
        bcBOR: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt16(left)^ or  PUInt16(right)^;
        bcXOR: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt16(left)^ xor PUInt16(right)^;
        bcSAR: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := Sar(PUInt16(left)^, PUInt16(right)^);
        bcSHR: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt16(left)^ shr PUInt16(right)^;
        bcSHL: PUInt16(Pointer(BasePtr + pc^.Args[2].Data.Addr))^ := PUInt16(left)^ shl PUInt16(right)^;
      end;

  end;
end;

